        "monthly"
      ]
    },
    "year": {
      "type": "string"
    },
    "month": {
      "type": "string"
    },
    "target_quantity": {
      "type": "biginteger"
    },
    "target_revenue_achieved": {
      "type": "biginteger"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "employee": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
